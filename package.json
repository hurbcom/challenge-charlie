{
    "name": "challenge-charlie",
    "version": "1.0.0",
    "description": "My Challenge To HurbCom",
    "main": "build/server/index.js",
    "scripts": {
        "test": "jest",
        "start": "docker compose -f docker-compose.prod.yml up",
        "start:prod": "NODE_ENV=production node build/server",
        "start:dev": "nodemon -L --exec node -- dev/server.js",
        "prebuild": "rm -rf .tmp/dev-react-cache && rm -rf build && rm -rf .swc/plugins",
        "build:react:client": "NODE_ENV=production webpack",
        "build:react:server": "NODE_ENV=production swc ./react -d build/react --config-file swc.config.json",
        "build:server": "NODE_ENV=production swc ./server -d build/server --config-file swc.config.json",
        "build:webpackConfig": "NODE_ENV=production swc ./webpack.config.ts -d build --config-file swc.config.json",
        "build": "npm run build:react:client && npm run build:react:server && npm run build:server && npm run build:webpackConfig",
        "dev": "docker compose -f docker-compose.dev.yml up"
    },
    "author": "Cesar de Barros <cesar@vaguarda.dev>",
    "license": "ISC",
    "engines": {
        "node": ">=18.14.2"
    },
    "devDependencies": {

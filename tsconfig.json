{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "baseUrl": "src"
  },
  "include": ["./src/**/*"]
}

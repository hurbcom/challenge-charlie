          "devServerTarget": "remotes-weather-forecast:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "src/apps/remotes/weather-forecast-e2e/**/*.{js,ts}"
        ]
      }

  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/apps/remotes/weather-forecast-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "src/apps/remotes/weather-forecast-e2e/cypress.config.ts",
        "devServerTarget": "remotes-weather-forecast:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
